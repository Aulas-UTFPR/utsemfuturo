{
    "version": 2,
    "build": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "command": "npm run build:prod",
          "outputDirectory": "."
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }